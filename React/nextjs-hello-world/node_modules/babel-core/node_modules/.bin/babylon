#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../styled-jsx/node_modules/babel-traverse/node_modules/babylon/bin/babylon.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../styled-jsx/node_modules/babel-traverse/node_modules/babylon/bin/babylon.js" "$@"
  ret=$?
fi
exit $ret
